  "rules": {
    "measurement": {
      "excision": {
        "formula": "excised_diameter = lesion_diameter + (2 × margin)",
        "common_margins_mm": {
          "benign": 1,
          "bcc_standard": 4,
          "scc_standard": 4,
          "melanoma_in_situ": 5,
          "melanoma_1mm": 10
        },
        "example": {
          "scenario": "6mm BCC with 4mm margins",
          "calculation": "6 + (2 × 4) = 14mm = 1.4cm",
          "code_selection": "Use 1.1-2.0cm size bracket"
        },
        "common_error": "Billing based on lesion size only, not excised specimen size"
      },
      "repair": {
        "formula": "Bill based on sutured wound length, including any dog ears",
        "common_error": "Measuring defect size instead of final sutured length"
      },
      "flap": {
        "formula": "total_area = primary_defect + secondary_defect(s)",
        "example": {
          "scenario": "Bilobed flap on nose",
          "calculation": "Primary 2 sq cm + First lobe 1.5 sq cm + Second lobe 0.8 sq cm = 4.3 sq cm",
          "code_selection": "14060 (≤10 sq cm)"
        },
        "common_error": "Only measuring primary defect - MISSES 50% of billable area"
      }
    },

    "repair_aggregation": {
      "rule": "Sum lengths of repairs with SAME complexity AND SAME anatomic group",
      "anatomic_groups": {
        "simple": {
          "group_A": ["scalp", "axilla", "trunk", "extremities"],
          "group_B": ["neck", "hands", "feet", "external genitalia"],
          "group_C": ["face", "ears", "eyelids", "nose", "lips", "mucous membranes"]
        },
        "intermediate": {
          "group_A": ["trunk", "extremities"],
          "group_B": ["scalp", "axilla", "neck", "hands", "feet", "external genitalia"],
          "group_C": ["face", "ears", "eyelids", "nose", "lips", "mucous membranes"]
        },
        "complex": {
          "group_A": ["trunk"],
          "group_B": ["scalp", "arms", "legs"],
          "group_C": ["forehead", "cheeks", "chin", "mouth", "neck", "axilla", "genitalia", "hands", "feet"],
          "group_D": ["eyelids", "nose", "ears", "lips"]
        }
      },
      "example": {
        "scenario": "2cm intermediate repair on cheek + 1.5cm intermediate repair on chin",
        "calculation": "Same complexity (intermediate) + Same group (face) = Sum lengths = 3.5cm",
        "code_selection": "12052 (2.6-5.0cm)"
      }
    },

